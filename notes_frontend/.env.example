# Frontend environment variables for the Notes app (React)

# Base URL for the FastAPI backend (include protocol and port)
# Example for local dev with backend on port 3001:
REACT_APP_API_BASE=http://localhost:3001

# Optional: if you expose separate envs in your platform, keep consistent values
REACT_APP_BACKEND_URL=$REACT_APP_API_BASE
REACT_APP_FRONTEND_URL=http://localhost:3000

# Optional runtime hints (not required)
REACT_APP_WS_URL=
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/health
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=false
